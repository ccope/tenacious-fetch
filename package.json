{
  "name": "tenacious-fetch",
  "version": "3.0.0-scaleai4",
  "description": "Tiny fetch API wrapper to add support for retries, retry delay & timeouts",
  "main": "dist/main.node.js",
  "browser": "dist/main.js",
  "source": "src/index.js",
  "repository": "https://github.com/tiaanduplessis/tenacious-fetch",
  "author": "Tiaan du Plessis",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "lint": "standard --fix ./**/*.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "prepublish": "yarn run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-loader": "^8.0.5",
    "babelify": "^10.0.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "express-async-router": "^0.1.15",
    "jest": "^22.4.3",
    "kill-port": "^1.2.0",
    "microbundle": "^0.4.4",
    "node-fetch": "^2.3.0",
    "standard": "^11.0.1",
    "stoppable": "^1.1.0"
  },
  "keywords": [
    "fetch",
    "fetch-wrapper",
